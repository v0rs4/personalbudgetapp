<!DOCTYPE html>
<html>
<head>
  <title>Personalbudget</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if flash[:notice] %>
   <div class="notice"><%= flash[:notice] %></div>
<% end %>

<% if flash[:error] %>
   <div class="error"><%= flash[:error] %></div>
<% end %>

<%= yield %>

</body>
</html>
